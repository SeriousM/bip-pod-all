        "properties": {
          "message": {
            "type": "text",
            "description": "Templated Message"
          }
        },
        "disposition": [
          "message"
        ]
      }
    },
    "markdown2html": {
      "title": "Markdown to HTML",
      "description": "Exports Markdown template parsed to HTML",
      "trigger": "invoke",
      "config": {
        "properties": {
          "message": {
            "type": "text",
            "description": "Default Message",
            "required": false
          }
        },
        "disposition": [
          "message"
        ]
      },
      "exports": {
        "properties": {
          "message": {
            "type": "string",
            "description": "Templated Markdown Message"
          }
        }
