      "imports": {
        "properties": {
          "message": {
            "type": "string",
            "description": "Templated Markdown Message"
          }
        },
        "disposition": [
          "message"
        ]
      }
    },
    "html2markdown": {
      "title": "HTML to Markdown",
      "description": "Converts a message from HTML to Markdown",
      "trigger": "invoke",
      "config": {
        "properties": {},
        "definitions": {}
      },
      "exports": {
        "properties": {
          "markdown": {
            "type": "string",
            "description": "Markdown"
          }
        }
      },
      "imports": {
        "properties": {
          "html": {
            "type": "string",
            "description": "HTML"
          }
        },
        "required": [
          "html"
        ],
        "disposition": [
          "html"
        ]
      }
    }
  }
