      "config": {
        "properties": {
          "message": {
            "type": "text",
            "description": "Default Message",
            "required": false
          }
        },
        "disposition": [
          "message"
        ]
      },
      "exports": {
        "properties": {
          "message": {
            "type": "text",
            "description": "Templated Message"
          }
