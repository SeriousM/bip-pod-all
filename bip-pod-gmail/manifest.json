{
  "name": "gmail",
  "title": "Gmail",
  "description": "Gmail is a Google-owned, web-based email service provides details of storage, options and links to related services",
  "url": "https://gmail.google.com",
  "trackDuplicates" : true,
  "config": {
    "oauth": {
      "clientID": "",
      "clientSecret": "",
      "scopes": [
        "https://www.googleapis.com/auth/gmail.compose",
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile",
        "https://www.googleapis.com/auth/gmail.readonly"
      ],
      "extras": {
        "accessType": "offline",
        "approvalPrompt": "force"
      }
    }
  },
  "auth": {
    "strategy": "oauth",
    "passport": {
      "provider": "google-oauth",
      "strategy": "OAuth2Strategy"
    },
    "properties": {
      "access_token": {
        "type": "string",
        "title": "Access Token"
      }
    },
    "disposition": [
      "access_token"
    ]
  },
  "rpcs": {
  },
  "actions": {
    "send": {
      "doc_ref" : "https://developers.google.com/gmail/api/v1/reference/",
      "title": "Send An Email",
      "description": "Send An Email via your Gmail account or alias",
      "trigger": "invoke",
      "config": {
        "properties": {
          "user_id" : {
            "type" : "string",
            "description" : "From Email",
            "default" : "me"
          }
        },
        "definitions": {},
        "disposition" : [ "user_id" ]
      },
      "exports": {
        "properties": {
          "id": {
            "type": "string",
            "description": "Message ID"
          },
          "threadId": {
            "type": "string",
            "description": "Thread ID"
          },
          "labelIds": {
            "type": "array",
            "description": "Labels"
          }
        }
      },
      "imports": {
        "properties": {
          "user_id" : {
            "type" : "string",
            "description" : "From Email"
          },
          "rcpt_to" : {
            "type" : "string",
            "description" : "To Email"
          },
          "reply_to" : {
            "type" : "string",
            "description" : "Reply To Email"
          },
          "subject" : {
            "type" : "string",
            "description" : "Subject"
          },
          "body" : {
            "type" : "string",
            "description" : "Body"
          }
        },
        "required" : [ "rcpt_to" ],
        "disposition" : [ "rcpt_to", "reply_to", "user_id", "subject", "body" ]
      }
    },
    "on_new_message": {
      "doc_ref" : "https://developers.google.com/gmail/api/v1/reference/",
      "title": "On A New Email",
      "description": "When A New Email Appears In Your Inbox",
      "trigger": "poll",
      "config": {
        "properties": {
        },
        "DISABLED_properties": {
          "get_attachments" : {
            "type" : "boolean",
            "description" : "Include Attachments",
            "default" : false
          }
        },
        "definitions": {}
      },
      "exports": {
        "DISABLED_properties": {
          "html_body": {
            "description": "HTML Body",
            "type": "string"
          }
        },
        "properties": {
          "text_body": {
            "description": "Text Body",
            "type": "string"
          },
          "mimeType": {
            "description": "Mime Type",
            "type": "string"
          },
          "Delivered-To": {
            "description": "Delivered To",
            "type": "string"
          },
          "Received": {
            "description": "Received",
            "type": "string"
          },
          "X-Received": {
            "description": "X Received",
            "type": "string"
          },
          "Return-Path": {
            "description": "Return Path",
            "type": "string"
          },
          "Received-SPF": {
            "description": "Received SPF",
            "type": "string"
          },
          "Authentication-Results": {
            "description": "Authentication Results",
            "type": "string"
          },
          "X-Google-DKIM-Signature": {
            "description": "X Google DKIM Signature",
            "type": "string"
          },
          "X-Gm-Message-State": {
            "description": "X Gm Message State",
            "type": "string"
          },
          "MIME-Version": {
            "description": "MIME Version",
            "type": "string"
          },
          "Reply-To": {
            "description": "Reply To",
            "type": "string"
          },
          "Date": {
            "description": "Date",
            "type": "string"
          },
          "Message-ID": {
            "description": "Message ID",
            "type": "string"
          },
          "Subject": {
            "description": "Subject",
            "type": "string"
          },
          "From": {
            "description": "From",
            "type": "string"
          },
          "To": {
            "description": "To",
            "type": "string"
          },
          "Content-Type": {
            "description": "Content Type",
            "type": "string"
          }
        }
      },
      "imports": {
        "properties": {
        }
      }
    }
  }
}