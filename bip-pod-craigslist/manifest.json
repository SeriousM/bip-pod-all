{
  "name": "craigslist",
  "title": "CraigsList",
  "description": "Craigslist provides local classifieds and forums for jobs, housing, for sale, personals, services, local community, and events",
  "url": "http://craigslist.org",
  "trackDuplicates": true,
  "config": {},
  "auth": {
    "strategy": "none"
  },
  "rpcs": {},
  "actions": {
    "search": {
      "title": "Listing Search",
      "description": "Exports the first 100 listings matching your search criteria",
      "doc_ref": "http://craigslist.org/search/sss?query=SearchString",
      "trigger": "invoke",
      "config": {
        "properties": {}
      },
      "exports": {
        "properties": {
          "pid": {
            "type": "string",
            "title": "PID"
          },
          "category": {
            "type": "string",
            "title": "Category"
          },
          "date": {
            "type": "string",
            "title": "Date"
          },
          "hasPic": {
            "type": "boolean",
            "title": "Has Picture"
          },
          "location": {
            "type": "string",
            "title": "Location"
          },
          "price": {
            "type": "string",
            "title": "Price"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "url": {
            "type": "string",
            "title": "Listing URL"
          }
        }
      },
      "imports": {
        "properties": {
          "city": {
            "type": "string",
            "title": "City"
          },
          "query": {
            "type": "string",
            "title": "Search"
          },
          "min": {
            "type": "string",
            "title": "Min Ask Price"
          },
          "max": {
            "type": "string",
            "title": "Max Ask Price"
          }
        },
        "required": [
          "city",
          "query"
        ],
        "disposition": [
          "city",
          "query",
          "min",
          "max"
        ]
      }
    },
    "on_match": {
      "title": "On A Matching Search",
      "description": "Triggers When A New Result Appears On Craigslist That Matches Your Search Terms",
      "doc_ref": "http://craigslist.org/search/sss?query=SearchString",
      "trigger": "poll",
      "imports": {
        "properties": {}
      },
      "exports": {
        "properties": {
          "pid": {
            "type": "string",
            "title": "PID"
          },
          "category": {
            "type": "string",
            "title": "Category"
          },
          "date": {
            "type": "string",
            "title": "Date"
          },
          "hasPic": {
            "type": "boolean",
            "title": "Has Picture"
          },
          "location": {
            "type": "string",
            "title": "Location"
          },
          "price": {
            "type": "string",
            "title": "Price"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "url": {
            "type": "string",
            "title": "Listing URL"
          }
        }
      },
      "config": {
        "properties": {
          "city": {
            "type": "string",
            "title": "City"
          },
          "query": {
            "type": "string",
            "title": "Search"
          },
          "min": {
            "type": "string",
            "title": "Min Ask Price"
          },
          "max": {
            "type": "string",
