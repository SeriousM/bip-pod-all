{
  "name": "tumblr",
  "title": "Tumblr",
  "description": "Tumblr is a microblogging platform and social networking website. The service allows users to post multimedia and other content to a short-form blog.",
  "url": "https://www.tumblr.com",
  "trackDuplicates": false,
  "config": {
    "oauth": {
      "consumerKey": "",
      "consumerSecret": ""
    }
  },
  "auth": {
    "strategy": "oauth"
  },
  "rpcs": {
    "user_info": {
      "contentType": "application/json",
      "properties": {
        "name": {
          "type": "string",
          "description": "Name"
        },
        "likes": {
          "type": "integer",
          "description": "Likes"
        },
        "following": {
          "type": "integer",
          "description": "Following"
        },
        "default_post_format": {
          "type": "string",
          "description": "Default Post Format"
        },
        "blogs": {
          "type": "array",
          "description": "List of Blogs"
        }
      },
      "title": "Get User Info"
    }
  },
  "actions": {
    "post_text": {
      "title": "New Text Post",
      "description": "New Text Post",
      "trigger": "invoke",
      "config": {
        "properties": {
          "url": {
            "type": "string",
            "description": "Blog URL eg: blog.tumblr.com",
            "optional": false,
            "unique": true,
            "oneOf": [
              {
                "$ref": "/renderers/user_info#user/blogs/{name}"
              }
            ]
          },
          "state": {
            "type": "string",
            "description": "Default State",
            "oneOf": [
              {
                "$ref": "#/config/definitions/state"
              }
            ]
          }
        },
        "definitions": {
          "state": {
            "description": "Post State",
            "enum": [
              "published",
              "draft",
              "queue",
              "private"
            ],
            "enum_label": [
              "Published",
              "Draft",
              "Queued",
              "Private"
            ],
            "default": "draft"
          }
        },
        "disposition": [
          "url",
          "state"
        ]
      },
      "exports": {
        "properties": {
          "id": {
            "type": "integer",
            "description": "Post ID"
          }
        }
      },
      "imports": {
        "properties": {
          "title": {
            "type": "string",
            "description": "Post Title"
          },
          "body": {
            "type": "string",
            "description": "Post Body"
          }
        },
        "disposition": [
          "title",
          "body"
        ]
      }
    },
